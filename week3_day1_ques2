import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;
import java.util.Scanner;
public class Solution {

    public static void main(String[] args) {
        Scanner obj1=new Scanner(System.in);
        int a,b,c;
        a=obj1.nextInt();
        b=a/10;
        System.out.println(a+b);
        c=((a+b)*100)/12;
        System.out.println(c/100F);
}}
